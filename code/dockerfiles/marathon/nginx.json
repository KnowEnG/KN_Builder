{
    "id": "p1nginx-TMPPORT",
    "instances": 1,
    "container": {
        "type": "DOCKER",
        "volumes": [{"containerPath": "/usr/share/nginx/html", "hostPath": "TMPPATH", "mode": "RO"},
                    {"containerPath": "/etc/nginx/conf.d/", "hostPath": "CONFPATH", "mode": "RW"}],
        "docker": {
            "image": "nginx",
            "network": "BRIDGE",
            "portMappings": [{"containerPort": 80, "hostPort": "TMPPORT"}]
        }
    }
}

{
    "id": "TMPID-c9",
    "args": ["--auth", "TMPID:knowdev249"],
    "env": {"PORT": "8181"},
    "cpus": 1.0,
    "mem": 1000,
    "instances": 1,
    "constraints": [["hostname", "CLUSTER", "knowcluster01.dyndns.org"]],
    "container": {
        "type": "DOCKER",
        "volumes": [
                    {"containerPath": "/var/run/docker.sock", "hostPath": "/var/run/docker.sock", "mode": "RW"},
                    {"containerPath": "/usr/bin/docker", "hostPath": "/usr/bin/docker", "mode": "RW"},
                    {"containerPath": "/workspace/knowtmp", "hostPath": "/mnt/knowtmp", "mode": "RW"},
                    {"containerPath": "/workspace/knowstorage", "hostPath": "/mnt/knowstorage", "mode": "RW"},
                    {"containerPath": "/workspace/knowdisk", "hostPath": "/mnt/knowdisk", "mode": "RW"},
                    {"containerPath": "/workspace/knowbackup", "hostPath": "/mnt/knowbackup", "mode": "RW"}
            ],
        "docker": {
            "image": "cblatti3/p1_cloud9:0.1",
            "network": "BRIDGE",
            "portMappings": [{"containerPort": 8181, "hostPort": TMPPORT}]
        }
    }
}
{
    "id": "p1mysql-TMPPORT",
    "cpus": 4.0,
    "mem": 16000,
    "instances": 1,
    "env": {"PORT": "3306"},    
    "constraints": [["hostname", "CLUSTER", "knowcluster05.dyndns.org"]],
    "container": {
        "type": "DOCKER",
        "volumes": [{"containerPath": "/var/lib/mysql", "hostPath": "/mnt/knowtmp/TMPPATH/", "mode": "RW"}],
        "docker": {
            "image": "mysql",
            "network": "BRIDGE",
            "parameters": [{ "key": "env", "value": "MYSQL_ROOT_PASSWORD=KnowEnG" }],
            "portMappings": [{"containerPort": 3306, "hostPort": TMPPORT}]
        }
    }
}



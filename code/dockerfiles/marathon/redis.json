{
    "id": "p1redis-TMPREDISPORT",
    "cmd": "redis-server --appendonly yes --requirepass TMPREDISPASS",
    "cpus": 1.0,
    "mem": "TMPREDISMEM",
    "instances": 1,
    "constraints": [["hostname", "CLUSTER", "TMPREDISHOST"]],
    "container": {
        "type": "DOCKER",
        "volumes": [{"containerPath": "/data", "hostPath": "TMPPATH/TMPREDISNAME/", "mode": "RW"}],
        "docker": {
            "image": "redis",
            "network": "HOST"
        }
    }
}

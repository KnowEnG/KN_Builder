{
    "id": "p1redis",
    "cmd": "redis-server --appendonly yes --requirepass KnowEnG",
    "cpus": 1.0,
    "mem": 16000,
    "instances": 1,
    "constraints": [["hostname", "CLUSTER", "knowcluster07.dyndns.org"]],
    "container": {
        "type": "DOCKER",
        "volumes": [{"containerPath": "/data", "hostPath": "/mnt/knowtmp/project1/p1_redis/", "mode": "RW"}],
        "docker": {
            "image": "redis",
            "network": "BRIDGE",
            "portMappings": [{"containerPort": 6379, "hostPort": 6379}]
        }
    }
}

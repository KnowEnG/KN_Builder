{
    "schedule": "R1\/\/P3M",
    "name": "TMPJOB",
    "container": {
        "type": "DOCKER",
        "image": "TMPIMG",
        "volumes": [{"containerPath": "/TMPDATAPATH/", "hostPath":"TMPDATADIR", "mode":"RW"},
                    {"containerPath": "/TMPCODEPATH/", "hostPath":"TMPCODEDIR", "mode":"RW"}]
    },
    "cpus": "1",
    "mem": "2500",
    "retries": "0",
    "command": "sh -c 'cd /TMPDATAPATH/TMPALIASPATH; /usr/bin/time -v python3 /TMPCODEPATH/fetch_utilities.py file_metadata.json TMPOPTS; TMPPIPECMD'" 
}
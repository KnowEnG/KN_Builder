{
    "schedule": "R1\/\/P3M",
    "name": "TMPJOB",
    "container": {
        "type": "DOCKER",
        "image": "TMPIMG",
        "volumes": [{"containerPath": "/TMPDATAPATH/", "hostPath":"TMPDATADIR", "mode":"RW"},
                    {"containerPath": "/TMPCODEPATH/", "hostPath":"TMPCODEDIR", "mode":"RW"}]
    },
    "cpus": "1",
    "mem": "512",
    "retries": "0",
    "command": "sh -c 'cd /TMPCODEPATH/; /usr/bin/time -v python3 /TMPCODEPATH/check_utilities.py TMPSRC TMPOPTS; TMPPIPECMD'"
}
{
    "schedule": "R1\/\/P3M",
    "name": "TMPJOB",
    "container": {
        "type": "DOCKER",
        "image": "TMPIMG",
        "volumes": [{"containerPath": "/TMPDATAPATH/", "hostPath":"TMPDATADIR", "mode":"RW"},
                    {"containerPath": "/TMPCODEPATH/", "hostPath":"TMPCODEDIR", "mode":"RW"}]
    },
    "cpus": "4",
    "mem": "2500",
    "retries": "0",
    "command": "sh -c '/usr/bin/time -v python3 /TMPCODEPATH/conv_utilities.py /TMPDATAPATH/TMPEDGEPATH TMPOPTS; TMPPIPECMD'"
}